<h1>New Lecture</h1>
<%= form_for [:cohort, @lecture] do |f| %>
  <%= f.label 'Title' %>
  <%= f.text_field :title %><br />
  <%= f.label 'Start Time' %>
  <%= f.datetime_field :start_at %><br />
  <%= f.label :duration %>
  <%= f.text_field :duration %> minutes<br />
  <%= f.collection_select :user_id, @instructors, :id, :email %> <br />
  <%= f.submit %>
<% end %>
